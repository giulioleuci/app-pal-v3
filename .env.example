# The title displayed in the browser tab and app header.
VITE_APP_TITLE="Blueprint Fitness"

# The version of the data schema for backups.
VITE_DATA_VERSION="1"

# The number of items to fetch per page in paginated lists.
VITE_PAGINATION_LIMIT="10"

# Default user settings
VITE_DEFAULT_THEME_MODE="dark"
VITE_DEFAULT_PRIMARY_COLOR="#90CAF9"
VITE_DEFAULT_SECONDARY_COLOR="#EF9A9A"
VITE_DEFAULT_UNIT_SYSTEM="metric"

# Default workout timers
VITE_DEFAULT_REST_SECONDS="90"

# Feature Flags
VITE_FEATURE_TRAINING_CYCLES_ENABLED="true"